{
    "name": "giftbox-www",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "NODE_ENV=development concurrently --restart-tries 10 --restart-after 1000 -k -c \"bgBlue.bold,bgBlack.bold\" npm:generate-watch npm:next:dev",
        "next:dev": "next dev",
        "build": "yarn generate && next build",
        "localstart": "yarn build &&  yarn start",
        "start": "next start",
        "_generate": "graphql-codegen --watch --config ./codegen-config.yml",
        "_apollo:generate": "apollo codegen:generate --includes=src/**/*.ts* --localSchemaFile ./generated-schema.graphql  --target typescript --tagName gql --outputFlat src/generated --excludes=src/generated/graphql.tsx",
        "_predev": "yarn generate",
        "generate": "graphql-codegen --config codegen.js",
        "generate-watch": "yarn generate --watch",
        "__generate": "apollo codegen:generate --watch --config=apollo.config.js --target typescript --outputFlat src/generated"
    },
    "dependencies": {
        "@apollo/client": "^3.0.0-rc.0",
        "@apollo/link-ws": "^2.0.0-beta.3",
        "@chakra-ui/core": "^0.8.0",
        "@emotion/core": "^10.0.28",
        "@emotion/styled": "^10.0.27",
        "computed-async-mobx": "^4.2.1",
        "date-fns": "^2.13.0",
        "easy-peasy": "^3.3.0",
        "emotion-theming": "^10.0.27",
        "isomorphic-unfetch": "^3.0.0",
        "lodash": "^4.17.19",
        "match-sorter": "^4.1.0",
        "memoize-one": "^5.1.1",
        "mobx": "^5.15.4",
        "mobx-hooks-form": "^0.2.7",
        "mobx-react-lite": "^2.0.6",
        "mobx-utils": "^5.5.7",
        "next": "11.1.3",
        "react": "16.13.1",
        "react-data-grid": "^7.0.0-canary.16",
        "react-datepicker": "^2.15.0",
        "react-dom": "16.13.1",
        "react-feather": "^2.0.8",
        "react-icons": "^3.10.0",
        "react-responsive": "^8.0.3",
        "react-table": "^7.0.4",
        "react-use": "^14.1.1",
        "reinspect": "^1.1.0",
        "wretch": "^1.7.2",
        "yup": "^0.28.5"
    },
    "devDependencies": {
        "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
        "@emotion/babel-preset-css-prop": "^10.0.27",
        "@graphql-codegen/cli": "^1.13.5",
        "@graphql-codegen/introspection": "^1.13.5",
        "@graphql-codegen/typescript": "^1.13.5",
        "@graphql-codegen/typescript-operations": "^1.13.5",
        "@graphql-codegen/typescript-react-apollo": "^1.13.5",
        "@testing-library/jest-dom": "^5.5.0",
        "@testing-library/react": "^10.0.3",
        "@types/lodash": "^4.14.150",
        "@types/node": "^13.13.2",
        "@types/react": "^16.9.34",
        "@types/react-datepicker": "^2.11.0",
        "@types/react-responsive": "^8.0.2",
        "@types/react-table": "^7.0.16",
        "@types/yup": "^0.28.3",
        "add": "^2.0.6",
        "apollo": "^2.27.3",
        "babel-plugin-emotion": "^10.0.33",
        "babel-plugin-macros": "^2.8.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "concurrently": "^5.2.0",
        "cypress": "^4.4.1",
        "cypress-dark": "^1.7.14",
        "eslint-plugin-cypress": "^2.10.3",
        "hooks.macro": "^1.1.1",
        "prettier": "^2.0.5",
        "typescript": "^3.9.2"
    }
}
